{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/OrganisationSummary.schema.json",
  "title": "Organisation summary",
  "description": "An organisation from the catalog",
  "type": "object",
  "properties": {
    "uri": {
      "$ref": "./OrganisationUri.schema.json"
    },
    "label": {
      "description": "The label of the organisation",
      "type": "string",
      "examples": ["developer.overheid.nl"]
    }
  },
  "required": ["uri", "label"]
}
