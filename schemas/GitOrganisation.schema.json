{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/GitOrganisation.schema.json",
  "title": "GitOrganisation",
  "description": "A git organisation from the catalog",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "readOnly": true
    },
    "organisation": {
      "$ref": "./OrganisationSummary.schema.json"
    },
    "url": {
      "$ref": "./GitOrganisationUrl.schema.json"
    }
  },
  "required": ["id", "organisation", "url"]
}
