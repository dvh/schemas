{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/CodeHosting.schema.json",
  "title": "CodeHosting",
  "description": "A code hosting from the catalog",
  "type": "object",
  "properties": {
    "url": {
      "type": "string",
      "format": "uri"
    },
    "group": {
      "type": "boolean",
      "default": true
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    }
  }
}
