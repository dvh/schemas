{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/Contact.schema.json",
  "title": "Contact",
  "description": "Contact details",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email",
      "description": "The email address of the contact",
      "examples": ["developer.overheid@geonovum.nl"]
    },
    "name": {
      "type": "string",
      "description": "The name of the contact",
      "examples": ["Developer Overheid"]
    },
    "url": {
      "type": "string",
      "format": "uri",
      "description": "The URL of the contact",
      "examples": ["https://developer.overheid.nl"]
    }
  },
  "required": ["email", "name", "url"]
}
