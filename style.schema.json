{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/style.schema.json",
  "title": "Style",
  "description": "A style from the catalog",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a style",
      "type": "integer"
    },
    "name": {
      "description": "The name of the style",
      "type": "string",
      "examples": ["Tripel", "Dubbel", "Blond"]
    }
  }
}